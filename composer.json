{
    "name": "gzero/admin",
    "description": "Gzero Admin Panel",
    "keywords": ["laravel", "admin panel", "gzero cms"],
    "license": "MIT",
    "authors": [
        {
            "name": "Adrian Skierniewski",
            "email": "adrian.skierniewski@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "5.0.*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.3.*"
    },
    "autoload": {
        "classmap": [
        ],
        "psr-4": {
            "Gzero\\": "src/Gzero"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan vendor:publish --tag=public --force"
        ],
        "post-update-cmd": [
            "php artisan vendor:publish --tag=public --force"
        ]
    },
    "minimum-stability": "stable"
}
